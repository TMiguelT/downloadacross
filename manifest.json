{
  "name": "Download a Cross",
  "description": "Browse crossword puzzles from the archives of all major crossword publishers",
  "version": "0.3.2",
  "permissions": [
    "storage",
    "activeTab",
    "downloads",
    "https://picayune.uclick.com/*",
    "https://www.nytimes.com/*",
    "https://blogs.wsj.com/*",
    "http://crosswordfiend.com/*",
    "http://herbach.dnsalias.com/*",
    "http://www.brendanemmettquigley.com/*",
    "http://www.chronicle.com/*"
  ],
  "browser_action": {
    "default_title": "Download a Cross",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
