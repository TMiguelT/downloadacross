{
  "name": "Download a Cross",
  "description": "Browse crossword puzzles from the archives of all major crossword publishers",
  "version": "0.4.0",
  "permissions": [
    "storage",
    "activeTab",
    "downloads"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_title": "Download a Cross",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}
